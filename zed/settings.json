{
  "context_servers": {
    "mcp-server-context7": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "default_minimum_tokens": "8000"
      }
    }
  },
  "current_line_highlight": "line",
  "active_pane_modifiers": {
    "border_size": 0.0,
    "inactive_opacity": 1.0
  },
  "show_edit_predictions": true,
  "icon_theme": "Warm Charmed Icons",
  "edit_predictions": {
    "mode": "eager"
  },
  "disable_ai": false,
  "theme": {
    "mode": "dark",
    "light": "Atomize",
    "dark": "One Dark Pro"
  },
  "tab_size": 2,
  "preferred_line_length": 80,
  "soft_wrap": "preferred_line_length",
  "show_wrap_guides": true,
  "wrap_guides": [80, 100],
  "buffer_font_size": null,
  "buffer_font_family": "Monaco",
  "ui_font_fallbacks": ["Inter", ".SystemUIFont"],
  "ui_font_size": 16,
  "vim_mode": false,
  "terminal": {
    "font_family": "Monaco",
    "blinking": "off",
    "font_size": 16,
    "working_directory": "current_project_directory",
    "dock": "bottom"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "file_finder": {
    "modal_max_width": "medium"
  },
  "tabs": {
    "close_position": "right",
    "git_status": true,
    "activate_on_close": "history",
    "show_close_button": "hover",
    "show_diagnostics": "errors"
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": true,
    "show_tab_bar_buttons": true
  },
  "auto_install_extensions": {
    "html": true,
    "astro": true,
    "atomize": true,
    "emmet": true,
    "env": true,
    "jsonnet": true,
    "toml": true
  },
  "use_autoclose": true,
  "format_on_save": "on",
  "autosave": "off",
  "formatter": [
    {
      "language_server": {
        "name": "eslint"
      }
    },
    {
      "external": {
        "command": "prettier",
        "arguments": ["--stdin-filepath", "{buffer_path}"]
      }
    }
  ],
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "journal": {
    "path": "~",
    "hour_format": "hour12"
  },
  "multi_cursor_modifier": "cmd_or_ctrl",
  "calls": {
    "mute_on_join": true,
    "share_on_join": false
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_project_items": true,
    "show_onboarding_banner": false,
    "show_user_picture": true,
    "show_sign_in": true,
    "show_menus": true
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "notification_panel": {
    "button": false
  },
  "agent": {
    "use_modifier_to_send": true,
    "play_sound_when_agent_done": true,
    "single_file_review": true,
    "profiles": {
      "ask": {
        "name": "Ask",
        "tools": {
          "contents": true,
          "diagnostics": true,
          "fetch": true,
          "list_directory": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "open": false,
          "grep": true,
          "thinking": true,
          "web_search": true
        },
        "enable_all_context_servers": false,
        "context_servers": {}
      },
      "minimal-zed": {
        "name": "Minimal - Zed",
        "tools": {
          "open": true,
          "find_path": true,
          "grep": true,
          "now": true,
          "read_file": true
        },
        "enable_all_context_servers": false,
        "context_servers": {
          "mcp-server-context7": {
            "tools": {
              "get-library-docs": true
            }
          },
          "mcp-server-sequential-thinking": {
            "tools": {
              "sequentialthinking": true
            }
          }
        }
      },
      "write": {
        "name": "Write",
        "tools": {
          "copy_path": true,
          "create_directory": true,
          "create_file": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "fetch": true,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "terminal": false,
          "thinking": true,
          "web_search": false,
          "open": true
        },
        "enable_all_context_servers": true,
        "context_servers": {}
      }
    },
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "default_width": 420,
    "inline_assistant_model": {
      "provider": "openai",
      "model": "gpt-4.1"
    },
    "commit_message_model": {
      "provider": "openai",
      "model": "gpt-4o-mini"
    },
    "thread_summary_model": {
      "provider": "openai",
      "model": "gpt-4o-mini"
    }
  },
  "git_panel": {
    "default_width": 240
  },
  "outline_panel": {
    "button": false,
    "default_width": 240,
    "dock": "left",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "indent_guides": {
      "show": "always"
    },
    "scrollbar": {
      "show": null
    }
  },
  "project_panel": {
    "button": true,
    "default_width": 200,
    "file_icons": true,
    "dock": "left",
    "folder_icons": true,
    "git_status": true,
    "indent_size": 14,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "show_diagnostics": "all",
    "scrollbar": {
      "show": "never"
    },
    "indent_guides": {
      "show": "never"
    }
  },
  "search": {
    "whole_word": true,
    "case_sensitive": true,
    "include_ignored": false,
    "regex": false
  },
  "seed_search_query_from_cursor": "selection",
  "completions": {
    "words": "enabled",
    "lsp": true,
    "lsp_fetch_timeout_ms": 0,
    "lsp_insert_mode": "replace_suffix"
  },
  "gutter": {
    "line_numbers": true,
    "runnables": false,
    "breakpoints": false,
    "folds": true,
    "min_line_number_digits": 1
  },
  "minimap": {
    "show": "never",
    "thumb": "hover",
    "thumb_border": "left_open",
    "current_line_highlight": "all"
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**/out",
    "**/dist",
    "**/.husky",
    "**/.turbo",
    "**/.vscode-test",
    "**/.vscode",
    "**/.next",
    "**/.storybook",
    "**/.tap",
    "**/.nyc_output",
    "**/node_modules"
  ]
}
